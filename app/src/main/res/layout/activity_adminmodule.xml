<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Adminmodule">

    <Button
        android:id="@+id/addst"
        android:layout_width="143dp"
        android:layout_height="116dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:background="@drawable/button_background"
        android:text="Add student"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.042" />

    <Button
        android:id="@+id/addrecruiter"
        android:layout_width="143dp"
        android:layout_height="116dp"
        android:background="@drawable/button_background"
        android:text="ADD RECRuiter"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.642"
        app:layout_constraintStart_toEndOf="@+id/addst"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.042" />

    <Button
        android:id="@+id/viewstudents"
        android:layout_width="143dp"
        android:layout_height="116dp"
        android:background="@drawable/button_background"
        android:text="view students"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addst"
        app:layout_constraintVertical_bias="0.024" />

    <Button
        android:id="@+id/viewrecruiters"
        android:layout_width="143dp"
        android:layout_height="116dp"
        android:background="@drawable/button_background"
        android:text="View recruiters"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.66"
        app:layout_constraintStart_toEndOf="@+id/viewstudents"
        app:layout_constraintTop_toBottomOf="@+id/addrecruiter"
        app:layout_constraintVertical_bias="0.024" />

    <Button
        android:id="@+id/drives"
        android:layout_width="143dp"
        android:layout_height="116dp"
        android:background="@drawable/button_background"
        android:text="Drives"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.082"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewstudents"
        app:layout_constraintVertical_bias="0.038" />

    <Button
        android:id="@+id/addmaterial"
        android:layout_width="143dp"
        android:layout_height="116dp"
        android:background="@drawable/button_background"
        android:text="ADD material"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.621"
        app:layout_constraintStart_toEndOf="@+id/drives"
        app:layout_constraintTop_toBottomOf="@+id/viewrecruiters"
        app:layout_constraintVertical_bias="0.038" />

    <Button
        android:id="@+id/selectedstudents"
        android:layout_width="145dp"
        android:layout_height="116dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:background="@drawable/button_background"
        android:text="Selected students"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/drives"
        app:layout_constraintVertical_bias="0.061" />

    <Button
        android:id="@+id/viewmaterials"
        android:layout_width="145dp"
        android:layout_height="116dp"
        android:background="@drawable/button_background"
        android:text="view all materials"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.626"
        app:layout_constraintStart_toEndOf="@+id/selectedstudents"
        app:layout_constraintTop_toBottomOf="@+id/addmaterial"
        app:layout_constraintVertical_bias="0.061" />

    <Button
        android:id="@+id/logout"
        android:layout_width="74dp"
        android:textColor="#ffffff"
        android:layout_height="30dp"
        android:background="@drawable/button_background"
        android:text="logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.977" />

</androidx.constraintlayout.widget.ConstraintLayout>